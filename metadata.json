{
  "name": "zleslie-nsd",
  "version": "0.3.7",
  "author": "zleslie",
  "summary": "Puppet NSD management module",
  "license": "Apache-2.0",
  "source": "git://github.com/xaque208/puppet-nsd.git",
  "project_page": "https://github.com/xaque208/puppet-nsd",
  "issues_url": "https://github.com/xaque208/puppet-nsd/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "5.5",
        "5.6",
        "5.7"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10.0",
        "10.1",
        "10.2"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 3.7.1"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/concat","version_requirement":">=1.2.1<2.0.0"},
    {"name":"puppetlabs/stdlib","version_requirement":">=4.4.0"}
  ]
}
